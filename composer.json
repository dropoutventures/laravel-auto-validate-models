{
    "name": "ryangjchandler/laravel-auto-validate-models",
    "description": "A small package to auto-validate your Laravel models.",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Chandler",
            "email": "support@ryangjchandler.co.uk",
            "homepage": "https://ryangjchandler.co.uk",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "RyanChandler\\AutoValidateModels\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RyanChandler\\AutoValidateModels\\AutoValidateModelsServiceProvider"
            ]
        }
    }
}
